{
    "name": "naucon/file",
    "description": "This package contains php classes to access, change, copy, delete, move, rename files and directories.",
    "keywords": ["file", "dir", "filesystem", "io", "copy", "move", "delete", "rename"],
    "homepage": "https://github.com/naucon/File",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Sanzenbacher"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "naucon/utility": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-4": {"Naucon\\File\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Naucon\\File\\Tests\\": "tests"}
    }
}
